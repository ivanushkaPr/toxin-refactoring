mixin UIColumnThree({gridThree, slider, inputDescriptionButtons, buttonContainerOne, buttonGradient, buttonGradientChecked, buttonContainerTwo, buttonTransparent, buttonTransarentChecked, buttonContainerThree, buttonText, buttonTextChecked, buttonWide, inputDescriptionPagination, pagination, inputDescriptionIdgSelected, inputDropdownGuestsSelected, bulletList})
  +grid(gridThree)
    +slider(slider)

    +input-description(inputDescriptionButtons)

    +button-container(buttonContainerOne)
      +button(buttonGradient)
      +button(buttonGradientChecked)

    +button-container(buttonContainerTwo)
      +button(buttonTransparent)
      +button(buttonTransarentChecked)
        
    +button-container(buttonContainerThree)
      +button(buttonText)
      +button(buttonTextChecked)

    +button-wide(buttonWide)

    +input-description(inputDescriptionPagination)
    +pagination(pagination)

    +input-description(inputDescriptionIdgSelected)
    +input-dropdown-guests(inputDropdownGuestsSelected)

    +bullet-list(bulletList)