mixin UIColumnTwo({gridTwo, checkboxButtons, inputDescriptionRadioButtons, radioButtons, inputDescriptionToggle, toggle, inputDescriptionLikeButton, likeButtonContainer, likeButtonOne, inputDescriptionRateButton, rateButtonOne, inputDescriptionIdp, inputDropdownGuests, richCheckboxButtons})
  +grid(gridTwo)
    +checkbox-buttons(checkboxButtons)
    +input-description(inputDescriptionRadioButtons)
    +radio-buttons(radioButtons)

    +input-description(inputDescriptionToggle)
    +toggle(toggle)

    +input-description(inputDescriptionLikeButton)
    +like-button-container(likeButtonContainer)
      +like-button(likeButtonOne)

    +input-description(inputDescriptionRateButton)
    div(class="rate-button__container")
      +rate-button(rateButtonOne)

    +input-description(inputDescriptionIdp)
    +input-dropdown-guests(inputDropdownGuests)

    +rich-checkbox-buttons(richCheckboxButtons)